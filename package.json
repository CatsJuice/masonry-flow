{
  "name": "masonry-flow",
  "version": "0.1.8",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter playground dev",
    "dev:react": "pnpm --filter playground dev",
    "dev:vue": "pnpm --filter vue-playground dev",
    "build": "node ./.build.mjs",
    "build:playground": "cd ./playground && tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "pub": "npm run build && npm publish"
  },
  "author": "CatsJuice",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/CatsJuice/masonry-flow"
  },
  "homepage": "https://masonry.oooo.so",
  "dependencies": {},
  "types": "dist/index.d.ts",
  "exports": {
    "./core": {
      "import": "./core/index.mjs",
      "types": "./core/index.d.ts",
      "default": "./core/index.mjs"
    },
    "./react": {
      "import": "./react/index.mjs",
      "types": "./react/index.d.ts",
      "default": "./react/index.mjs"
    },
    "./vue": {
      "import": "./vue/index.mjs",
      "types": "./vue/index.d.ts",
      "default": "./vue/index.mjs"
    }
  },
  "keywords": [
    "masonry",
    "absolute grid",
    "masonry layout"
  ],
  "devDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@tweakpane/core": "^2.0.4",
    "@types/node": "^20.12.4",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "brotli-size": "^4.0.0",
    "chalk": "^5.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "execa": "^9.3.0",
    "pretty-bytes": "^6.1.1",
    "prompts": "^2.4.2",
    "shx": "^0.3.4",
    "tslib": "^2.6.3",
    "tweakpane": "^4.0.4",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "4.0.0-beta.1",
    "vue": "^3.4.31"
  }
}